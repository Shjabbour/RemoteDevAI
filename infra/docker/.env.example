# Docker Environment Variables
# Copy this file to .env and adjust values as needed

# Environment
NODE_ENV=development

# PostgreSQL Database
POSTGRES_DB=remotedevai
POSTGRES_USER=remotedevai
POSTGRES_PASSWORD=remotedevai_dev_password
POSTGRES_PORT=5432

# Redis Cache
REDIS_PORT=6379
REDIS_PASSWORD=remotedevai_redis_password

# MinIO (S3-compatible storage)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001

# S3 Configuration
S3_BUCKET=remotedevai-files
S3_REGION=us-east-1

# Cloud Backend
CLOUD_PORT=3000
WS_PORT=3001

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:5173

# Logging
LOG_LEVEL=debug

# Desktop Agent (for testing)
CLOUD_URL=http://cloud:3000
AGENT_ID=test-agent-1
AGENT_TOKEN=test-token
